# Supabase Configuration Template
# Copy this file to .env and fill in your actual values

# Your Supabase project URL (found in Settings > API)
SUPABASE_URL=https://your-project-ref.supabase.co

# Your Supabase anon/public key (found in Settings > API)
SUPABASE_ANON_KEY=your_anon_key_here

# Your Supabase service role key (found in Settings > API) - keep this secret!
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database connection string (found in Settings > Database)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your-project-ref.supabase.co:5432/postgres

# Optional: Additional configuration
ENVIRONMENT=development
DEBUG=true
